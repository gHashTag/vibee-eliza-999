{
  "name": "@elizaos/plugin-redpill",
  "version": "1.2.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos-plugins/plugin-redpill.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ai-sdk/openai": "^1.1.9",
    "@ai-sdk/ui-utils": "^1.2.8",
    "@elizaos/core": "^1.2.5",
    "ai": "^4.3.9",
    "js-tiktoken": "^1.0.18",
    "tsup": "8.4.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "prettier --write ./src",
    "clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json tsconfig.tsbuildinfo",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "REDPILL_API_KEY": {
        "type": "string",
        "description": "API key used to authenticate requests to the RedPill service.",
        "required": false,
        "sensitive": true
      },
      "REDPILL_BASE_URL": {
        "type": "string",
        "description": "Base URL for the RedPill API endpoints.",
        "required": false,
        "default": "https://api.red-pill.ai/v1",
        "sensitive": false
      },
      "REDPILL_SMALL_MODEL": {
        "type": "string",
        "description": "Identifier of the small language model to use with RedPill.",
        "required": false,
        "default": "gpt-4o-mini",
        "sensitive": false
      },
      "SMALL_MODEL": {
        "type": "string",
        "description": "Fallback identifier of the small language model if REDPILL_SMALL_MODEL is not set.",
        "required": false,
        "default": "gpt-4o-mini",
        "sensitive": false
      },
      "REDPILL_LARGE_MODEL": {
        "type": "string",
        "description": "Identifier of the large language model to use with RedPill.",
        "required": false,
        "default": "gpt-4o",
        "sensitive": false
      },
      "LARGE_MODEL": {
        "type": "string",
        "description": "Fallback identifier of the large language model if REDPILL_LARGE_MODEL is not set.",
        "required": false,
        "default": "gpt-4o",
        "sensitive": false
      }
    }
  },
  "gitHead": "646c632924826e2b75c2304a75ee56959fe4a460",
  "devDependencies": {
    "prettier": "3.5.3",
    "typescript": "5.8.2"
  }
}
